def transpose_matrix(A):
    rows = len(A)
    cols = len(A[0])
    transpose = [[A[j][i] for j in range(rows)] for i in range(cols)]

    print("Original Matrix:")
    for row in A:
        print(row)

    print("\nTranspose:")
    for row in transpose:
        print(row)

# Example
A = [
    [1, 2, 3],
    [4, 5, 6]
]

transpose_matrix(A)
